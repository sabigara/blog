{
  "name": "tailwind-nextjs-starter-blog",
  "version": "1.5.1",
  "private": true,
  "scripts": {
    "start": "cross-env SOCKET=true node ./scripts/next-remote-watch.js ./data",
    "dev": "next dev --port 3700",
    "build": "next build",
    "vercel-build": "next build && next-sitemap",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,md,mdx}'",
    "prepare": "husky install",
    "compose": "node ./scripts/compose.js",
    "run-ts": "node -r esbuild-register"
  },
  "dependencies": {
    "@fontsource/inter": "4.5.11",
    "@headlessui/react": "^1.7.7",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "clsx": "^1.2.1",
    "contentlayer": "^0.2.9",
    "github-slugger": "^1.3.0",
    "gray-matter": "^4.0.2",
    "i18next": "^22.4.6",
    "image-size": "1.0.2",
    "js-yaml": "^4.1.0",
    "jsdom": "^20.0.0",
    "mdast-util-to-string": "^3.1.0",
    "mdx-bundler": "^9.0.1",
    "next": "^13.0.5",
    "next-contentlayer": "^0.2.9",
    "next-i18next": "^13.0.2",
    "next-sitemap": "^3.1.44",
    "next-themes": "^0.2.0",
    "postcss": "^8.4.5",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.1.1",
    "react-icons": "^4.4.0",
    "reading-time": "1.5.0",
    "rehype": "^12.0.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-citation": "^0.4.0",
    "rehype-external-links": "^2.0.1",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.1.3",
    "rehype-slug": "^5.0.0",
    "remark": "^14.0.2",
    "remark-directive": "^2.0.1",
    "remark-footnotes": "^4.0.1",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "rss-parser": "^3.12.0",
    "sass": "^1.56.1",
    "sharp": "^0.30.7",
    "smoothscroll-polyfill": "^0.4.4",
    "tailwindcss": "^3.0.18",
    "unist-util-visit": "^4.0.0",
    "vfile": "^5.3.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.2.3",
    "@octokit/rest": "^19.0.3",
    "@svgr/webpack": "^6.1.2",
    "@types/js-yaml": "^4.0.5",
    "@types/jsdom": "^20.0.0",
    "@types/prettier": "^2.6.4",
    "@types/react": "^18.0.15",
    "@types/tailwindcss": "^3.1.0",
    "@types/unist": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.21.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.0.0",
    "globby": "13.1.2",
    "husky": "^8.0.1",
    "inquirer": "^8.0.0",
    "lint-staged": "^13.0.3",
    "next-remote-watch": "^1.0.0",
    "node-fetch": "^3.2.10",
    "prettier": "^2.5.1",
    "prettier-plugin-md-nocjsp": "^1.3.0",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "socket.io": "^4.4.0",
    "socket.io-client": "^4.4.0",
    "typescript": "~4.9.3",
    "webpack": "^5.74.0"
  },
  "packageManager": "yarn@1.22.15",
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
